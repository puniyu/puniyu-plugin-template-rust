{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>puniyu/.github//.github/renovate"
  ],
  "packageRules": [
    {
      "groupName": "puniyu",
      "matchDatasources": [
        "crate"
      ],
      "matchPackageNames": [
        "puniyu_core",
        "puniyu_plugin"
      ],
      "rangeStrategy": "auto",
      "automerge": true,
      "automergeType": "pr"
    }
  ]
}
